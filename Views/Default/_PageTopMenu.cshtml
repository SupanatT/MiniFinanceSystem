
@model MiniFinance.Models.LoginModel.Login



<div class="page-header-inner ">
    <div class="page-logo">
        <a href="#">
            <img src="@Url.Content("~/img/logo.png")" alt="logo" class="logo-default" />
        </a>
        <div class="menu-toggler sidebar-toggler"> </div>
    </div>
    <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse"> </a>
    <div class="top-menu">
        <ul class="nav navbar-nav pull-right">

           



            <li class="dropdown dropdown-user">
                <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                    <img alt="" class="img-circle" style="height:29px;width:29px;" src="@Url.Content("~/img/Default/default_avatar.png")" id="userAvatar" />
                    <span class="username username-hide-on-mobile" id="userName"> @Html.ValueFor(x=>x.username) | @Html.ValueFor(x=>x.branchDisplayName) </span>
                    <i class="fa fa-angle-down"></i>
                </a>
                <ul class="dropdown-menu dropdown-menu-default">
                    <li>
                       
                        <ul class="dropdown-menu">
                            <li class="external">
                                <h3>
                                    <span class="bold">12 pending</span> notifications
                                </h3>
                                <a href="page_user_profile_1.html">view all</a>
                            </li>
                        </ul>
                    </li>
                    <li class="divider"> </li>
                    <li>
                        <a href="#" onclick="LogOut()">
                            <i class="icon-key"></i> Log Out
                        </a>
                    </li>
                </ul>
            </li>

        </ul>
    </div>
</div>

<script>
    function LogOut() {
        $.ajax({
            url: '@Url.Action("LogOut", "Default")',
            type: 'POST',
            dataType: 'json',
            data: null,
            success: function (e) {
                alert('Logout Success');
                window.location = e.url;
            }
        })
    }

    function Setting(param) {
        $.ajax({
            url: '@Url.Action("Setting", "Default")',
            type: 'POST',
            dataType: 'json',
            data: { param: param },
            success: function (e) {

            }
        })
    }
</script>



